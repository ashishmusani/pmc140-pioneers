IsValid] = useState(false);\n    const [isMarked, setIsMarked] = useState((name !== \"\") && (email !== \"\"));\n\n    useEffect(()=>{\n        console.log(isMarked)\n    }, [])\n\n    const validateEmail = (emailId) => {\n        const reg = /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n        if(reg.test(String(emailId).toLowerCase())) {\n            setEmailIsValid(true)\n        } else {\n            setEmailIsValid(false)\n        }\n    }\n\n    const handleNameChange = (e) => {\n        setName(e.target.value)\n    }\n    const handleEmailChange = (e) => {\n        const email = e.target.value;\n        setEmail(email)\n        validateEmail(email);\n    }\n\n    const handleSubmit = () =>{\n        setIsProcessing(true);\n        if(name.length>0 && emailIsValid){\n            const attendeeDetails = {\n                \"attendeeName\": name,\n                \"attendeeEmail\": email\n              }        \n            //axios.post(`${window.location.href}login`, userCredentials)\n            axios.post('http://localhost:5000/api/attendees', attendeeDetails)\n            .then(res => {\n                if(res.status === 200){\n                    alert(\"Success! You have been marked present.\")\n                    setName(\"\");\n                    setEmail(\"\");\n                    localStorage.setItem(\"name\", name);\n                    localStorage.setItem(\"email\", email);\n                    setIsProcessing(false);\n                    setIsMarked(true);\n                } else {\n                    alert(\"There was a problem! Please try again.\")\n                    setIsProcessing(false);\n                }\n            })\n            .catch(err => {\n                alert(\"There was a problem! Please try again.\")\n                setIsProcessing(false);\n            })\n            \n        } else {\n            alert(\"Error! Invalid data provided.\");\n            setIsProcessing(false);\n        }\n    }\n    \n    // const clearLocalStorage = () => {\n    //     localStorage.removeItem(\"name\");\n    //     localStorage.removeItem(\"email\");\n    // }\n\n    // clearLocalStorage();\n\n    return isMarked? (`Hi ${name}! You have been marked present`) : (\n        <Form id=\"attendancePage\" >\n            <Form.Group className=\"justify-content-center\" as={Row}>\n                <Form.Label column sm=\"3\" md=\"1\" lg=\"1\">\n                    Full Name\n                </Form.Label>\n                <Col sm=\"5\" md=\"4\" lg=\"4\">\n                    <Form.Control \n                        type=\"text\" \n                        placeholder=\"Name as in seneca records\"\n                        value={name}\n                        onChange = {handleNameChange}\n                        disabled={isProcessing}  \n                    />\n                    <Form.Text>\n                        {!name.length>0? \"Enter a valid name\" : null}\n                    </Form.Text>\n                </Col>\n            </Form.Group>\n            <Form.Group className=\"justify-content-center\" as={Row}>\n                <Form.Label column sm=\"3\" md=\"1\" lg=\"1\">\n                    Email\n                </Form.Label>\n                <Col sm=\"5\" md=\"4\" lg=\"4\">\n                    <Form.Control \n                    type=\"email\" \n                    placeholder=\"Seneca email id\"\n                    value={email}\n                    onChange = {handleEmailChange}  \n                    disabled={isProcessing}  \n                />\n                    <Form.Text>\n                        {!emailIsValid? \"Enter a valid email id\" : null}\n                    </Form.Text>\n                </Col>\n            </Form.Group>\n            <Form.Group as={Row} className=\"justify-content-center\">\n                <Col sm=\"8\" md=\"10\" lg=\"5\">\n                    <Button \n                        className=\"btn-block\" \n                        onClick={handleSubmit}\n                        disabled={isProcessing}  \n                    >\n                        {isProcessing? \"Please wait...\" : \"Mark Me Present\"}\n                    </Button>\n                </Col>\n            </Form.Group>\n        </Form>        \n    )\n}\n\nexport default AttendancePage;","/home/ashish/ReactJs/LeadershipSymposium/client/src/components/QuestionPage/index.js",["82","83","84","85","86"],"/home/ashish/ReactJs/LeadershipSymposium/client/src/components/AdminPage/index.js",[],"/home/ashish/ReactJs/LeadershipSymposium/client/src/components/common/Header.js",["87"],"/home/ashish/ReactJs/LeadershipSymposium/client/src/components/common/PageNotFound.js",["88"],"import React from 'react';\n\nconst PageNotFound = () => {\n\n    const pageStyle={\n        paddingTop: \"10vmin\",\n        textAlign: \"center\"\n    }\n\n    const imageStyle={\n        width: \"80vmin\"\n    }\n\n    return (\n        <div style={pageStyle} id=\"pageNotFound\" >\n            <img style={imageStyle} src=\"https://www.joulin.com/images/404.png\"/>\n        </div>        \n    )\n}\n\nexport default PageNotFound;","/home/ashish/ReactJs/LeadershipSymposium/client/src/components/AdminPage/AdminDashboard/index.js",["89","90"],"/home/ashish/ReactJs/LeadershipSymposium/client/src/components/AdminPage/AdminDashboard/Tabs/PollsTab.js",[],"/home/ashish/ReactJs/LeadershipSymposium/client/src/components/AdminPage/AdminDashboard/Tabs/AttendeesTab.js",[],"/home/ashish/ReactJs/LeadershipSymposium/client/src/components/AdminPage/AdminDashboard/Tabs/QuestionsTab.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":9,"column":8,"nodeType":"97","messageId":"98","endLine":9,"endColumn":25},{"ruleId":"95","severity":1,"message":"99","line":10,"column":8,"nodeType":"97","messageId":"98","e